import java.util.*;
class Solution {
    public String solution(String s) {
        String answer = "";
        //1. 주어진 문자열을 한글자씩 배열로 저장
        String []sArr = s.split(" ");
        //2. 문자열을 숫자로 변환하여 저장할 배열생성
        int []nArr = new int[sArr.length];
        for(int i = 0 ; i < sArr.length; i++){
            //3. 문자열 한글자씩 숫자로 변환하여 2번에서 선언한 배열에 저장
            nArr[i] = Integer.valueOf(sArr[i]);
        }
        //4. 배열을 sort함수 이용하여 정렬
        Arrays.sort(nArr);
        //5. 배열 index 0 과 제일 끝 index 출력
        return nArr[0]+" "+nArr[nArr.length-1];
    }
}
